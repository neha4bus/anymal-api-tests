<?xml version='1.0' encoding='utf-8'?>
<package format="3">

  <name>anymal_api_proto</name>
  <version>0.8.8</version>
  <description>Protobuf definitions for the ANYmal API.</description>

  <license>Proprietary</license>

  <author email="ynava@anybotics.com">Yoshua Nava</author>
  <author email="hwyss@anybotics.com">Hannes Wyss</author>
  <author email="ghottiger@anybotics.com">Gabriel Hottiger</author>

  <!-- The API gatekeepers - alphabetical order -->
  <maintainer email="apoghosov@anybotics.com">Armen Poghosov</maintainer>
  <maintainer email="jcravo@anybotics.com">Jo√£o Cravo</maintainer>
  <maintainer email="jvelasco@anybotics.com">Javier De Velasco Oriol</maintainer>
  <maintainer email="pmorier@anybotics.com">Philippe Morier</maintainer>
  <maintainer email="rdiethelm@anybotics.com">Remo Diethelm</maintainer>

  <buildtool_depend condition="$ROS_VERSION == 1">catkin</buildtool_depend>
  <buildtool_depend condition="$ROS_VERSION == 2">ament_cmake</buildtool_depend>

  <!-- System dependencies required for building and executing -->
  <depend condition="$ROS_VERSION == 1">catkin</depend> <!-- provides the /opt/ros/noetic/setup.bash file -->

  <!-- Catkin dependencies required for building only -->
  <build_depend condition="$ROS_VERSION == 1">cmake_clang_tools</build_depend>
  <build_depend condition="$ROS_VERSION == 1">pkg-config</build_depend>

  <export>
    <build_type condition="$ROS_VERSION == 2">ament_cmake</build_type>
  </export>
</package>
